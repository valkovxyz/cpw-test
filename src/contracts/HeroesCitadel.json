  "metadata": "{\"compiler\":{\"version\":\"0.8.17+commit.8df45f5f\"},\"language\":\"Solidity\",\"output\":{\"abi\":[{\"inputs\":[{\"internalType\":\"address\",\"name\":\"_battleStation\",\"type\":\"address\"}],\"stateMutability\":\"nonpayable\",\"type\":\"constructor\"},{\"inputs\":[],\"name\":\"InvalidSessionWallet\",\"type\":\"error\"},{\"anonymous\":false,\"inputs\":[{\"indexed\":true,\"internalType\":\"address\",\"name\":\"hero\",\"type\":\"address\"},{\"indexed\":true,\"internalType\":\"address\",\"name\":\"owner\",\"type\":\"address\"}],\"name\":\"HeroCreated\",\"type\":\"event\"},{\"inputs\":[],\"name\":\"battleStation\",\"outputs\":[{\"internalType\":\"address\",\"name\":\"\",\"type\":\"address\"}],\"stateMutability\":\"view\",\"type\":\"function\"},{\"inputs\":[{\"internalType\":\"string\",\"name\":\"_name\",\"type\":\"string\"},{\"internalType\":\"address\",\"name\":\"_sessionWallet\",\"type\":\"address\"},{\"internalType\":\"enum HeroClass\",\"name\":\"_heroClass\",\"type\":\"uint8\"},{\"internalType\":\"enum StatsKey[]\",\"name\":\"_keys\",\"type\":\"uint8[]\"},{\"internalType\":\"uint16[]\",\"name\":\"_values\",\"type\":\"uint16[]\"}],\"name\":\"createHero\",\"outputs\":[],\"stateMutability\":\"payable\",\"type\":\"function\"},{\"inputs\":[{\"internalType\":\"address\",\"name\":\"_owner\",\"type\":\"address\"}],\"name\":\"getHeroes\",\"outputs\":[{\"internalType\":\"address[]\",\"name\":\"\",\"type\":\"address[]\"}],\"stateMutability\":\"view\",\"type\":\"function\"}],\"devdoc\":{\"kind\":\"dev\",\"methods\":{},\"version\":1},\"userdoc\":{\"kind\":\"user\",\"methods\":{},\"version\":1}},\"settings\":{\"compilationTarget\":{\"contracts/citadel/HeroesCitadel.sol\":\"HeroesCitadel\"},\"evmVersion\":\"london\",\"libraries\":{},\"metadata\":{\"bytecodeHash\":\"ipfs\",\"useLiteralContent\":true},\"optimizer\":{\"enabled\":true,\"runs\":200},\"remappings\":[]},\"sources\":{\"contracts/battle-station/interfaces/IBattleHero.sol\":{\"content\":\"// SPDX-License-Identifier: BUSL-1.1\\npragma solidity 0.8.17;\\n\\nimport \\\"../../hero/HeroDataTypes.sol\\\";\\n\\ninterface IBattleHero {\\n    /// @dev only owner can set stats\\n    function setStats(StatsKey[] calldata _keys, uint16[] calldata _values) external;\\n\\n    function initHero() external;\\n\\n    function getStats(address _hero) external view returns (uint16[16] memory);\\n\\n    function availableStats(address _hero) external view returns (uint256);\\n\\n    function getHeroDetails(address _hero) external view returns (HeroDetails memory);\\n\\n    function getXP(address _hero) external view returns (uint32);\\n\\n    function getLevel(address _hero) external view returns (uint32);\\n\\n    function getHP(address _hero) external view returns (uint64);\\n\\n    function getDamageAsInt256(address _hero) external view returns (int256);\\n}\\n\",\"keccak256\":\"0xae275f7919e29e48549a9df7115a5c7726a29cd5806240eb0c4e847116b973f6\",\"license\":\"BUSL-1.1\"},\"contracts/citadel/HeroesCitadel.sol\":{\"content\":\"// SPDX-License-Identifier: BUSL-1.1\\npragma solidity 0.8.17;\\n\\nimport \\\"../interfaces/IHeroesCitadel.sol\\\";\\nimport \\\"../hero/Hero.sol\\\";\\n\\ncontract HeroesCitadel is IHeroesCitadel {\\n    address public battleStation;\\n\\n    mapping(address => address[]) internal _heroes;\\n\\n    event HeroCreated(address indexed hero, address indexed owner);\\n\\n    error InvalidSessionWallet();\\n\\n    constructor(address _battleStation) {\\n        battleStation = _battleStation;\\n    }\\n\\n    function createHero(\\n        string calldata _name,\\n        address _sessionWallet,\\n        HeroClass _heroClass,\\n        StatsKey[] calldata _keys,\\n        uint16[] calldata _values\\n    )\\n        external\\n        payable\\n    {\\n        address hero = address(new Hero(\\n            battleStation,\\n            msg.sender,\\n            _name,\\n            _heroClass,\\n            _keys,\\n            _values\\n        ));\\n\\n        _heroes[msg.sender].push(hero);\\n\\n        if (msg.value != 0) {\\n            if (_sessionWallet == address(0)) revert InvalidSessionWallet();\\n            payable(_sessionWallet).transfer(msg.value);\\n        }\\n\\n        emit HeroCreated(hero, msg.sender);\\n    }\\n\\n    function getHeroes(address _owner) external view returns (address[] memory) {\\n        return _heroes[_owner];\\n    }\\n}\\n\",\"keccak256\":\"0x3d5438ae82129ce7b8088b909bc1717742162a522f427aca9280237874d698fb\",\"license\":\"BUSL-1.1\"},\"contracts/hero/Hero.sol\":{\"content\":\"// SPDX-License-Identifier: BUSL-1.1\\npragma solidity 0.8.17;\\n\\nimport \\\"../interfaces/IHero.sol\\\";\\nimport \\\"../battle-station/interfaces/IBattleHero.sol\\\";\\n\\ncontract Hero is IHero {\\n    address public battleStation;\\n    HeroClass public heroClass;\\n    string internal _name;\\n    address internal _owner;\\n\\n    error NoAllowed();\\n    error InvalidHeroClass();\\n\\n    event StatAdded(StatsKey key, uint16 value);\\n\\n    modifier onlyBattleStation() {\\n        if(msg.sender != battleStation) revert NoAllowed();\\n        _;\\n    }\\n\\n    modifier onlyOwner() {\\n        if(msg.sender != _owner) revert NoAllowed();\\n        _;\\n    }\\n\\n    constructor(\\n        address _battleStation,\\n        address _creator,\\n        string memory _heroName,\\n        HeroClass _heroClass,\\n        StatsKey[] memory _keys,\\n        uint16[] memory _values\\n    ) {\\n        if (_heroClass == HeroClass.NOT_CONFIGURED) revert InvalidHeroClass();\\n\\n        _name = _heroName;\\n        _owner = _creator;\\n        battleStation = _battleStation;\\n        heroClass = _heroClass;\\n\\n        IBattleHero(_battleStation).initHero();\\n\\n        _setStats(_keys, _values);\\n    }\\n\\n    /// @dev only owner can set stats\\n    function setStats(StatsKey[] memory _keys, uint16[] memory _values) external onlyOwner {\\n        _setStats(_keys, _values);\\n    }\\n\\n    function getStats() external view returns (uint16[16] memory _result) {\\n        return IBattleHero(battleStation).getStats(address(this));\\n    }\\n\\n    function availableStats() external view returns (uint256) {\\n        return IBattleHero(battleStation).availableStats(address(this));\\n    }\\n\\n    function getHeroDetails() external view returns (HeroDetails memory) {\\n        return IBattleHero(battleStation).getHeroDetails(address(this));\\n    }\\n\\n    function getOwner() external view returns (address) {\\n        return _owner;\\n    }\\n\\n    function name() external view returns (string memory) {\\n        return _name;\\n    }\\n\\n    function _setStats(StatsKey[] memory _keys, uint16[] memory _values) internal {\\n        IBattleHero(battleStation).setStats(_keys, _values);\\n\\n        for (uint256 i = 0; i < _keys.length; i++) {\\n            emit StatAdded(_keys[i], _values[i]);\\n        }\\n    }\\n}\\n\",\"keccak256\":\"0x565d869c016ea7d67cb3a12193b8a964b410532af7b1f628d7476cea41ade3e9\",\"license\":\"BUSL-1.1\"},\"contracts/hero/HeroDataTypes.sol\":{\"content\":\"// SPDX-License-Identifier: BUSL-1.1\\npragma solidity 0.8.17;\\n\\nenum HeroClass {\\n    NOT_CONFIGURED,\\n    INFILTRATOR,\\n    AGENT_X,\\n    TECHNOMANCER,\\n    NETRUNNER\\n}\\n\\nenum StatsKey {\\n    NotConfigured,\\n    Attack,\\n    Health\\n}\\n\\nstruct HeroDetails {\\n    uint32 wins;\\n    uint32 loses;\\n    uint32 draws;\\n    uint32 level;\\n    uint32 xp;\\n}\\n\",\"keccak256\":\"0x523950e451e7f5aebe57dfc10cde36ececf2829fa5af8a095425c705e69a5b8b\",\"license\":\"BUSL-1.1\"},\"contracts/interfaces/IHero.sol\":{\"content\":\"// SPDX-License-Identifier: BUSL-1.1\\npragma solidity 0.8.17;\\n\\nimport \\\"../hero/HeroDataTypes.sol\\\";\\n\\ninterface IHero {\\n    /// @dev only owner can set stats\\n    function setStats(StatsKey[] calldata _keys, uint16[] calldata _values) external;\\n\\n    function getStats() external view returns (uint16[16] memory);\\n\\n    function getOwner() external view returns (address);\\n\\n    function availableStats() external view returns (uint256);\\n\\n    function getHeroDetails() external view returns (HeroDetails memory);\\n\\n    function name() external view returns (string memory);\\n}\\n\",\"keccak256\":\"0x078cd475792b6007142f49d7a3f9267c1e644560af6164be89e3ca1126e2553b\",\"license\":\"BUSL-1.1\"},\"contracts/interfaces/IHeroesCitadel.sol\":{\"content\":\"// SPDX-License-Identifier: BUSL-1.1\\npragma solidity 0.8.17;\\n\\nimport \\\"../hero/HeroDataTypes.sol\\\";\\n\\ninterface IHeroesCitadel {\\n    function createHero(\\n        string calldata _name,\\n        address _sessionWallet,\\n        HeroClass _heroClass,\\n        StatsKey[] calldata _keys,\\n        uint16[] calldata _values\\n    )\\n        external\\n        payable;\\n}\\n\",\"keccak256\":\"0x8e8f3c8e6442aafaa88234caf9df623af5ed700173b6b21133227d49897a31c6\",\"license\":\"BUSL-1.1\"}},\"version\":1}",
